    <servlet-name>Partie</servlet-name>
    <url-pattern>/Partie</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Seance</servlet-name>
    <url-pattern>/Seance</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Juge</servlet-name>
    <url-pattern>/Juge</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Jury</servlet-name>
    <url-pattern>/Jury</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Avocat</servlet-name>
