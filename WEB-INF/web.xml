<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app id="test-biblio">
  <display-name>Syst√®me de gestion d'un palais de justice</display-name>
  <context-param>
    <param-name>exempleParam1</param-name>
    <param-value>valeurDeExempleParam1</param-value>
  </context-param>
  <context-param>
    <param-name>exempleParam2</param-name>
    <param-value>valeurDeExempleParam2</param-value>
  </context-param>
      <servlet>
        <servlet-name>jsp</servlet-name>
        <servlet-class>org.apache.jasper.servlet.JspServlet</servlet-class>
        <init-param>
            <param-name>fork</param-name>
            <param-value>false</param-value>
        </init-param>
        <init-param>
            <param-name>xpoweredBy</param-name>
            <param-value>false</param-value>
        </init-param>
        <init-param>
            <param-name>javaEncoding</param-name>
            <param-value>ISO-8859-1</param-value>
        </init-param>
        <load-on-startup>3</load-on-startup>
    </servlet>
  <listener>
    <listener-class>JusticeServlet.JusticeSessionListener</listener-class>
    <listener-class>JusticeServlet.JusticeContextListener</listener-class>
  </listener>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>JusticeServlet.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Accueil</servlet-name>
    <servlet-class>JusticeServlet.Accueil</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>JusticeServlet.Logout</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Proces</servlet-name>
    <servlet-class>JusticeServlet.Proces</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Partie</servlet-name>
    <servlet-class>JusticeServlet.Partie</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Seance</servlet-name>
    <servlet-class>JusticeServlet.Seance</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Juge</servlet-name>
    <servlet-class>JusticeServlet.Juge</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Jury</servlet-name>
    <servlet-class>JusticeServlet.Jury</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Avocat</servlet-name>
    <servlet-class>JusticeServlet.Avocat</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Accueil</servlet-name>
    <url-pattern>/Accueil</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/Logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Proces</servlet-name>
    <url-pattern>/Proces</url-pattern>
  </servlet-mapping>
